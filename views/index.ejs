									var requestFullScreen = elem.requestFullscreen || elem.mozRequestFullScreen || elem.webkitRequestFullscreen || elem.msRequestFullscreen;
									if (requestFullScreen) {
											requestFullScreen.call(elem);
									}
									var audio = new Audio('assets/audio/Take That - Patient.mp3'); // Gantilah dengan path ke file audio Anda
									await audio.play();

									// Event listener untuk memulai ulang audio saat selesai diputar
									audio.addEventListener('ended', function() {
											audio.currentTime = 0; // Mengatur waktu audio kembali ke awal
											audio.play(); // Memulai ulang audio
									});
							}
					});
			});
